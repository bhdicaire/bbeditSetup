    <array>
      <string>alias</string>
      <string>audit</string>
      <string>before</string>
      <string>check</string>
      <string>loglevel</string>
      <string>noop</string>
      <string>notify</string>
      <string>require</string>
      <string>schedule</string>
      <string>stage</string>
      <string>subscribe</string>
      <string>tag</string>
