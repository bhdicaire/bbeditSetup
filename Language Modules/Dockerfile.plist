		<!-- from https://github.com/poblish/Docker.bbpackage -->
    <key>Function Pattern</key>
    <string><![CDATA[
      (?x:
          (?P<leadspace>^\s*)
          (?P<function>
              [^#]?RUN\s+(?P<function_name>\S+)
              (?:(?![A-Z]+\s+)(?:.+)+(?:\R))+
          )
      )
    ]]>
