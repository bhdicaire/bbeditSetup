<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!--
  Extremely simple ARM asm language module. Most keywords are missing, I
  only added the few that actually turned up in files I was looking at.
  
  TODO: Take an ARM reference, and add all opcodes
-->
<plist version="1.0">
<dict>
	<key>BBEditDocumentType</key>
	<string>CodelessLanguageModule</string>
	<key>BBLMLanguageDisplayName</key>
	<string>ARM Assembler</string>
	<key>BBLMLanguageCode</key>
	<string>ARMA</string>
	<key>BBLMColorsSyntax</key>
	<true/>
	<key>BBLMIsCaseSensitive</key>
	<false/>
	<key>BBLMScansFunctions</key>
	<false/>
	<key>BBLMKeywordList</key>
	<array>
		<string>ADD</string>
		<string>ADDGE</string>
		<string>ADDS</string>
		<string>B</string>
		<string>BGE</string>
		<string>BGT</string>
		<string>BLE</string>
		<string>BLT</string>
		<string>CMP</string>
		<string>LDMFD</string>
		<string>LDMIA</string>
		<string>LDR</string>
		<string>LDRH</string>
		<string>LDRSH</string>
		<string>MLA</string>
		<string>MOV</string>
		<string>MUL</string>
		<string>ORR</string>
		<string>RSCVS</string>
		<string>STMFD</string>
		<string>STMIA</string>
		<string>STRH</string>
		<string>SUB</string>
		<string>SUBS</string>
		<string>.text</string>
		<string>.global</string>
		<string>r0</string>
		<string>r1</string>
		<string>r2</string>
		<string>r3</string>
		<string>r4</string>
		<string>r5</string>
		<string>r6</string>
		<string>r7</string>
		<string>r8</string>
		<string>r9</string>
		<string>r10</string>
		<string>r11</string>
		<string>r12</string>
		<string>r13</string>
		<string>r14</string>
	</array>
	<key>BBLMSuffixMap</key>
	<array>
		<dict>
			<key>BBLMLanguageSuffix</key>
			<string>.s</string>
		</dict>
	</array>
	<key>Language Features</key>
	<dict>
		<key>Identifier and Keyword Character Class</key>
		<string>[:alnum:]</string>
		<key>Open Line Comments</key>
		<string>@</string>
		<key>Open Strings 1</key>
		<string>&apos;</string>
		<key>Close Strings 1</key>
		<string>&apos;</string>
		<key>Escape Char in Strings 1</key>
		<string></string>
		<key>End-of-line Ends Strings 1</key>
		<true/>
		<key>Open Strings 2</key>
		<string>&quot;</string>
		<key>Close Strings 2</key>
		<string>&quot;</string>
		<key>Escape Char in Strings 2</key>
		<string></string>
		<key>End-of-line Ends Strings 2</key>
		<true/>
	</dict>
	<key>BBLMCommentLineDefault</key>
	<string>@ </string>
	<key>com.barebones.DocumentType</key>
	<string>CodelessLanguageModule</string>
</dict>
</plist>
